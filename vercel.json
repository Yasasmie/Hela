{
  "version": 2,
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    { "source": "/admin/(.*)", "destination": "/api/index.js" },
    { "source": "/packages", "destination": "/api/index.js" },
    { "source": "/ads", "destination": "/api/index.js" }
  ]
}
